<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="user_syndic_management" model="ir.ui.view">
        <field name="name">Syndic Management user</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
             <xpath expr="//page" position="after">
    				    <page string="Information Syndic">
    					    <group>
    						      <field name="server_mail_id"/>
                  </group>
		            </page>
             </xpath>
        </field>
  	</record>
</odoo>

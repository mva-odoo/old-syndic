<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="syndic_calendar_res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.syndic.calendar</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="syndic_base.syndic_res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='module_syndic_calendar']/../.." position="after">
                    <div class="col-xs-12 col-md-12 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="month_delay"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="month_delay"/>
                        </div>
                    </div>
                </xpath>
            </field>
    </record>
</odoo>